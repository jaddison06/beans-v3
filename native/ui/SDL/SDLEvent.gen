@LinkWithLib(SDL2)

@Prefix(SE)
class SDLEventRaw {
    @Initializer()
    void* SEInit()

    @Invalidates()
    void SEDestroy()

    void SEGetPos(int* x, int* y)
    @Getter(key)
    KeyCode SEGetKey()
    @Getter(mouseButton)
    MouseButton SEGetMouseButton()
    @Getter(type)
    EventType SEGetType()

    bool SEHasShift()
    bool SEHasControl()
    bool SEHasAlt()
    bool SEHasCaps()

    int SEPoll()
}